public class Thing2 extends LinkedList {
    public Thing2() {}
    
    public void put(Object o) {
        addFirst(o);
    }
    
    public Object get() {
        if (!this.isEmpty()) {
            return removeFirst();
        } else {
            System.err.println("You can\'t do that!");
            return null;
        } 
    }
    
    public Object peek() {
        return getFirst();
    }
}